    <release-date>2019-04-30</release-date>

    <variant id="workstation">
      <_name>Fedora 30 Workstation</_name>
    </variant>
    <variant id="workstation-netinst">
      <_name>Fedora 30 Workstation</_name>
    </variant>
    <variant id="server">
