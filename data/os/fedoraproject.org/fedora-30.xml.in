  <os id="http://fedoraproject.org/fedora/30">
    <short-id>fedora30</short-id>
    <_name>Fedora 30</_name>
    <version>30</version>
    <_vendor>Fedora Project</_vendor>
    <family>linux</family>
    <distro>fedora</distro>
    <upgrades id="http://fedoraproject.org/fedora/29"/>
